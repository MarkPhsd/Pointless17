{
	"appId": "www.pointlesspos.com",
	"appName": "pointlesspos",
	"bundledWebRuntime": false,
	"npmClient": "npm",
	"webDir": "",
	"plugins": {
		"SplashScreen": {
			"launchShowDuration": 0,
			"launchAutoHide": true
		},
		"Keyboard": {
			"resize": "body",
			"style": "dark"
		}
	},
	"cordova": {
		"preferences": {
			"ScrollEnabled": "true",
			"BackupWebStorage": "none",
			"SplashMaintainAspectRatio": "true",
			"FadeSplashScreenDuration": "300",
			"SplashShowOnlyFirstTime": "false",
			"SplashScreen": "screen",
			"SplashScreenDelay": "3000"
		}
	},
  "server": {
    "hostname": "Pointless",
    "iosScheme": "ionic",
    "androidScheme": "http",
    "url": "https://pointlesspos.web.app"
  }
}

