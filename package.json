  "scripts": {
    "ng": "ng",
    "start with IP": "ng serve --configuration dev --public-host --host=192.168.0.6 --port=4200 ",
    "startLocal": "ng serve --configuration dev --host=localhost --port=4200",
    "startLocalNode": "ng serve   --configuration dev --host=localhost --port=4200",
    "buildHighMem production": "node --max_old_space_size=16384 ./node_modules/@angular/cli/bin/ng build  --configuration production",
    "BuildCoconuts": "node --max_old_space_size=16384 ./node_modules/@angular/cli/bin/ng build --configuration production ",
    "BuildKosher": "node --max_old_space_size=16384 ./node_modules/@angular/cli/bin/ng build --configuration productionKosher",
    "BuildPointless": "node --max_old_space_size=16384 ./node_modules/@angular/cli/bin/ng build --configuration productionPointless",
    "BuildPointless Dev": "node --max_old_space_size=16384 ./node_modules/@angular/cli/bin/ng build --configuration dev --source-map=true",
    "FireBaseKosher": "firebase deploy --only hosting:ockosherstore",
    "FireBasePointless": "firebase deploy --only hosting:pointlesspos",
    "FireBasecoconutsfl": "firebase deploy --only hosting:coconutsfl",
    "ElectrDist": "npx cap copy electron",
    "buildHighMemDebug": "node --max_old_space_size=16384 ./node_modules/@angular/cli/bin/ng build --configuration dev --outputHashing=all",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compodoc": "./node_modules/.bin/compodoc -p tsconfig.json -w -s",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook"
  },
